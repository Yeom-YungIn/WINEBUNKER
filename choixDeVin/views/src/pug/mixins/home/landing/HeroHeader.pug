mixin HeaderProductCard(config)
  img(src=`${CWD}assets/img/gallery/${config.image}`  alt='products').w-100

mixin HeroHeader({id})
  section.py-0#header
    .container
      .row.align-items-center.min-vh-75.min-vh-md-50
        .col-12.text-center.py-8
          h1.lh-sm.fs-lg-6.fs-xxl-7 Choix De Vin
            br
            |
          p.text-700 Choose your favorite wine && Record the price of the wine you bought
          .mt-5
          div(class= 'search')
            input(type="text" class="searchTerm" placeholder= "검색어를 입력해주세요")
            button(type="submit" class= "searchButton")
              i 검색
    .container-fluid  
      .row.gx-2 
        .col-12    
          .swiper-container.pb-4.overflow-hidden(data-pagination-target=id) 
            .swiper-wrapper
              each item in headerProducts
                .swiper-slide.h-auto
                  +HeaderProductCard(item) 
            //- .swiper-pagination(id=id)